  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dumpsayamrat/express-es6-server-starter/issues"
  },
  "homepage": "https://github.com/dumpsayamrat/express-es6-server-starter#readme",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-2": "^6.22.0",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1"
  },
  "dependencies": {
    "express": "^4.15.2"
  }
}
